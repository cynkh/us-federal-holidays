    <GenerateProgramFile>false</GenerateProgramFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore" Version="1.1.7"/>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="1.1.8"/>
